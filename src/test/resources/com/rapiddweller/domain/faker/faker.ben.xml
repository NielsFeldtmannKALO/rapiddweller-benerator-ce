        <attribute name="weather_description" generator="new FakerGenerator('weather','description')"/>
        <attribute name="witcher_location" generator="new FakerGenerator('witcher','location')"/>
        <attribute name="yoda_quote" generator="new FakerGenerator('yoda','quote')"/>
        <attribute name="zelda_character" generator="new FakerGenerator('zelda','character')"/>
    </generate>

    <echo>Check if enough data has been generated</echo>
    <iterate type="faker_data" source="mem" consumer="ConsoleExporter">
        <attribute name="counter" type="int" generator="IncrementGenerator"/>
    </iterate>
    <if test="faker_data.counter != 100">
        <error>Not enough data has been generated</error>
    </if>
</setup>