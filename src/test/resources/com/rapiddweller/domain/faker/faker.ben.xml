        <attribute name="buffy_quotes" generator="new FakerGenerator('buffy','quotes')"/>
        <attribute name="business_creditCardNumber" generator="new FakerGenerator('business','creditCardNumber')"/>
        <attribute name="cat_name" generator="new FakerGenerator('cat','name')"/>
        <attribute name="chuckNorris_fact" generator="new FakerGenerator('chuckNorris','fact')"/>
        <attribute name="code_asin" generator="new FakerGenerator('code','asin')"/>
